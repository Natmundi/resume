<main>
  <div>
    {{#with page_header}}
      <h1 class='text-center tex-dark'>{{title}}</h1>
      <p
        class='text-center text-capitalize'
      >{{description}}</p>
    {{/with}}
    {{#with section}}
      <div class='card'>
        <div class='card-body'>
          {{#with about_us}}
            {{#with title_list}}
              <div class='display-6 text-end'>{{main}}</div>
              <h2 class='text-end'>{{sub}}</h2>
            {{/with}}
            <br />
            {{#with button}}
              <button
                class='btn btn-outline-info'
              >{{text}}</button>
            {{/with}}
          {{/with}}
        </div>
      </div>
      <br />
      <div class='card'>
        <div class='card-body'>
          {{#with advantage}}
            <div class='display-5'>{{title}}</div>
            <br />
            <div class='card'>
              <div class='card-body'>
                {{#with efficiency}}
                  <div class='display-4'>{{title}}</div>
                  <h6 class='text-dark'>{{sub_title}}</h6>
                  <p class='text-dark'>{{description}}</p>
                  <br />
                  <button
                    class='btn btn-outline-warning'
                  >{{button}}</button>
                {{/with}}
              </div>
            </div>
            <br />
            <br />
            <div class='card'>
              <div class='card-body'>
                {{#with innovation}}
                  <div
                    class='display-4 text-center'
                  >{{title}}</div>
                  <h6
                    class='text-dark text-center'
                  >{{sub_title}}</h6>
                  <p
                    class='text-dark text-center'
                  >{{description}}</p>
                  <br />
                  <button
                    class='btn btn-outline-warning'
                  >{{button}}</button>
                {{/with}}
              </div>
            </div>
            <br />
            <br />
            <div class='card'>
              <div class='card-body'>
                {{#with collaboration}}
                  <div
                    class='display-4 text-end'
                  >{{title}}</div>
                  <h6
                    class='text-dark text-end'
                  >{{sub_title}}</h6>
                  <p
                    class='text-dark text-end'
                  >{{description}}</p>
                  <br />
                  <button
                    class='btn btn-outline-warning'
                  >{{button}}</button>
                {{/with}}
              </div>
            </div>
          {{/with}}
        </div>
      </div>
      <br />
      <br />
      <div class='card'>
        <div class='card-body'>
          {{#with agreement}}
            <div class='display-5'>{{title}}</div>
            <div class='card'>
              <div class='card-body'>
                {{#with info}}
                  <h5 class='text-dark'>{{sub_title}}</h5>
                  <div class='card'>
                    <div class='card-body'>
                      {{#with detail}}
                        <p class='text-dark'>{{text}}</p>
                        <button
                          class='btn btn-success'
                        >{{button}}</button>
                      {{/with}}
                    </div>
                  </div>
                {{/with}}
              </div>
            </div>
          {{/with}}
        </div>
      </div>

    {{/with}}

  </div>
</main>