<main>
  <div class='row gx-5'>
    <section class='col-6'>

      {{#with navigation}}
        {{#each links}}
          <a
            href='{{href}}'
            class='bth btn-link'
          >{{text}}</a>
        {{/each}}
        {{#with button}}
          <a
            href='{{href}}'
            class='btn btn-primary'
          >{{text}}</a>
        {{/with}}
      {{/with}}

    </section>
  </div>

</main>